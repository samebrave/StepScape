<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <font
        android:font="@font/sansation_light"
        android:fontStyle="normal"
        android:fontWeight="300"
        app:font="@font/sansation_light"
        app:fontStyle="normal"
        app:fontWeight="300" />

    <font
        android:font="@font/sansation_light_italic"
        android:fontStyle="italic"
        android:fontWeight="300"
        app:font="@font/sansation_light_italic"
        app:fontStyle="italic"
        app:fontWeight="300" />

    <font
        android:font="@font/sansation_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/sansation_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <font
        android:font="@font/sansation_italic"
        android:fontStyle="italic"
        android:fontWeight="400"
        app:font="@font/sansation_italic"
        app:fontStyle="italic"
        app:fontWeight="400" />

    <font
        android:font="@font/sansation_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/sansation_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />

    <font
        android:font="@font/sansation_bold_italic"
        android:fontStyle="italic"
        android:fontWeight="700"
        app:font="@font/sansation_bold_italic"
        app:fontStyle="italic"
        app:fontWeight="700" />

</font-family>
